#include<stdio.h>
void main()
{
int num=1303,originalnum,rem,result=0;
originalnum=num;
while(originalnum!=0)
{remainder=originalnum%10;
result+=rem*rem*rem*rem;
orinalnum/=10;
}
if(result==num)
printf("%d is a armstrong number\n,num");
else
printf("%d is not an armstrong number\n");
getch();
}
