#include<stdio.h>
void main()
{
int num=121,reversed=0,rem,originalnum;
originalnum=num;
while(num!=0)
{
rem=num%10;
reversed=reversed*10+rem;
num/=10;
}
if(originalnum==reversed)
{
prinf("%d is a palindrome",originalnum);
else
printf("%d  is not a palindrome",originalnum);
}
getch();
}
